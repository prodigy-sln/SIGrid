{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "System": "Warning",
        "Microsoft": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] <{SourceContext}> {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Telemetry": {
    "Enabled": true,
    "Influx": {
      "Host": "",
      "Organization": "",
      "Bucket": "",
      "ApiKey": ""
    }
  },
  "SIGrid": {
    "TradedSymbols": [
      {
        "Exchange": "OKX",
        "SymbolType": "Swap",
        "Symbol": "BTC-USD-SWAP",
        "InvestPerGrid": 1.0,
        "ReinvestProfit": 1.0,
        "Leverage": 5.0,
        "TakeProfitPercent": 0.05,
        "MaxActiveBuyOrders": 3
      }
    ]
  },
  "OKX": {
    "ApiKey": "",
    "ApiSecret": "",
    "ApiPassPhrase": ""
  }
}
